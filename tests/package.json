    "test:windows": "mocha tests/windows/*.js --timeout 5000",
    "test:unix": "mocha tests/unix/*.js --timeout 5000",
    "test:python": "python -m pytest tests/ -v",
    "test:python:common": "python -m pytest tests/common/ -v",
    "test:python:windows": "python -m pytest tests/windows/ -v",
    "test:python:unix": "python -m pytest tests/unix/ -v"
  },
  "keywords": ["ctypes", "ffi", "test"],
