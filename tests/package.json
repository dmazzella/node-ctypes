{
  "name": "node-ctypes-tests",
  "version": "0.1.0",
  "description": "Test suite for node-ctypes",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "node --test **/test_*.js --timeout 5000",
    "test:common": "node --test common/test_*.js --timeout 5000",
    "test:windows": "node --test windows/test_*.js --timeout 5000",
    "test:unix": "node --test unix/test_*.js --timeout 5000",
    "test:python": "python -Bm pytest . -v",
    "bench:koffi": "node benchmarks/benchmark_koffi.js",
    "bench:python": "python benchmarks/benchmark_python.py"
  },
  "dependencies": {
    "node-ctypes": ".."
  },
  "devDependencies": {
    "koffi": "^2.15.0"
  }
}
