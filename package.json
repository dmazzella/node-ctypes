{
  "name": "node-ctypes",
  "version": "0.1.2",
  "description": "Python ctypes-like FFI for Node.js using libffi",
  "author": "",
  "license": "MIT",
  "keywords": [
    "ffi",
    "ctypes",
    "native",
    "libffi"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "build": "cmake-js compile --target ctypes",
    "build:debug": "cmake-js compile --target ctypes --debug",
    "rebuild": "cmake-js rebuild --target ctypes",
    "clean": "cmake-js clean",
    "generate-ffi-headers": "node deps/libffi_cmake/generate-headers.js"
  },
  "devDependencies": {
    "cmake-js": "^7.4.0",
    "node-addon-api": "^8.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
