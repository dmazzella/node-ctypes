{
  "name": "node-ctypes",
  "version": "0.1.0",
  "description": "Python ctypes-like FFI for Node.js using libffi",
  "author": "",
  "license": "MIT",
  "keywords": [
    "ffi",
    "ctypes",
    "native",
    "libffi"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "install": "cmake-js compile --target ctypes",
    "build": "cmake-js compile --target ctypes",
    "build:debug": "cmake-js compile --debug",
    "rebuild": "cmake-js rebuild",
    "clean": "cmake-js clean"
  },
  "devDependencies": {
    "@napi-ffi/ffi-napi": "^4.0.3",
    "cmake-js": "^7.4.0",
    "koffi": "^2.15.0"
  },
  "dependencies": {
    "node-addon-api": "^8.5.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
